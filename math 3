// User function Template for Java

class Solution {
    int MissingNumber(int array[], int n) {
        // Your Code Here
       
        int sum=Arrays.stream(array).sum();
        int sum1=(n*(n+1))/2;
        
        return sum1-sum;
        
    }
}
