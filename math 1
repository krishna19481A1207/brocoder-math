https://practice.geeksforgeeks.org/problems/prime-number2314/1



class Solution{
    static int isPrime(int N){
        // code here
        int n=N,c=0;
        if(n<=1) return 0;
        for(int i=2;i<=Math.sqrt(n);i++) if(n%i==0) c++;
        if(c==0) return 1;
        else return 0;
        
        
    }
}

